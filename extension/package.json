{
  "name": "stet",
  "displayName": "Stet",
  "description": "Local-first code review tool (Cursor)",
  "version": "0.0.1",
  "publisher": "stet",
  "engines": {
    "vscode": "^1.85.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:stet.startReview"
  ],
  "contributes": {
    "commands": [
      {
        "command": "stet.startReview",
        "title": "Stet: Start review"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.0",
    "typescript": "^5.3.0"
  }
}
